{
  "name": "Take a break",
  "version": "1.0",
  "description": "Lets take a break and embrace the moment",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    }
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "background": {
    "scripts": ["index.js"],
    "persistent": false
  },
  "manifest_version": 2
}
